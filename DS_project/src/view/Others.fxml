<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="577.0" prefWidth="568.0" styleClass="FXMLDocument" stylesheets="@css.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OthersController">
   <children>
      <Label prefHeight="115.0" prefWidth="536.0" text="How Can I Help You?" textFill="#85eddd">
         <font>
            <Font name="Lucida Bright Demibold" size="49.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <RadioButton fx:id="fd" mnemonicParsing="false" onAction="#FixedDeposit" text="Fixed Deposit">
         <textFill>
            <LinearGradient endX="1.0" endY="0.7809523809523811" startX="1.0" startY="0.719047619047619">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="#94928c" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </RadioButton>
      <RadioButton fx:id="chkbal" mnemonicParsing="false" onAction="#checkbalance" prefHeight="22.0" prefWidth="150.0" selected="true" text="Check Balance">
         <textFill>
            <LinearGradient endX="1.0" endY="0.7809523809523811" startX="1.0" startY="0.719047619047619">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="#94928c" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin>
      </RadioButton>
      <RadioButton fx:id="uinfo" mnemonicParsing="false" onAction="#updateinformation" text="Profit Information">
         <textFill>
            <LinearGradient endX="1.0" endY="0.7809523809523811" startX="1.0" startY="0.719047619047619">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="#94928c" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin>
      </RadioButton>
      <Button fx:id="exit" mnemonicParsing="false" onAction="#exit" opacity="0.73" prefHeight="57.0" prefWidth="247.0" style="-fx-background-color: #d3cbcb;" text="BACK">
         <font>
            <Font name="Bauhaus 93" size="16.0" />
         </font>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.9047619047619048">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="#6f6c6c" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_CENTER" prefHeight="657.0" prefWidth="818.0" styleClass="FXMLDocument" stylesheets="@css.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLDocumentController">
   <children>
      <Label fx:id="label" minHeight="16" minWidth="69" prefHeight="195.0" prefWidth="798.0" text="Welcome To Online Banking System" textFill="#94a6a8">
         <font>
            <Font name="System Bold" size="46.0" />
         </font>
      </Label>
      <Button fx:id="admin" onAction="#handleAdminAction" prefHeight="41.0" prefWidth="177.0" style="-fx-background-color: #07489d; -fx-opacity: 0.8;" text="ADMIN" textFill="#94a6a8">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
         <VBox.margin>
            <Insets top="100.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="button" onAction="#handleButtonAction" prefHeight="51.0" prefWidth="173.0" style="-fx-background-color: #07489d; -fx-opacity: 0.8;" text="USER" textFill="#94a6a8">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="exit" onAction="#exit" prefHeight="50.0" prefWidth="178.0" style="-fx-background-color: #07489d; -fx-opacity: 0.8;" text="EXIT" textFill="#94a6a8">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
